  },
  "keywords": [
    "mcp",
    "plugin",
    "postgres",
    "postgresql"
  ],
  "author": "nam088",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21.1",
    "@nam088/mcp-core": "^0.2.9",
    "pg": "^8.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/pg": "^8.11.10",
    "typescript": "^5.9.3"
  }
}
