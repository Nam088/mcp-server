{
  "name": "@nam088/mcp-postgres",
  "version": "0.2.1",
  "description": "PostgreSQL plugin for MCP server",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "mcp-postgres": "dist/bin/server.js"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "files": [
